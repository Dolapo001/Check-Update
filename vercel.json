{
  "version": 3,
  "builds": [
    {
      "src": "*/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "*/wsgi.py"
    }
  ],
  "buildCommand": "chmod +x build.sh && ./build.sh"
}